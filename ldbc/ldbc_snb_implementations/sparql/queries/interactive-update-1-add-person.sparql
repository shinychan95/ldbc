PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sn: <http://www.ldbc.eu/ldbc_socialnet/1.0/data/>
PREFIX snvoc: <http://www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/>
PREFIX sntag: <http://www.ldbc.eu/ldbc_socialnet/1.0/tag/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dbpedia: <http://dbpedia.org/resource/>
PREFIX dbpedia-owl: <http://dbpedia.org/ontology/>

INSERT
{
    sn:pers$personId a snvoc:Person .
    sn:pers$personId snvoc:id "$personId"^^xsd:long .
    sn:pers$personId snvoc:firstName $personFirstName .
    sn:pers$personId snvoc:lastName $personLastName .
    sn:pers$personId snvoc:gender $gender .
    sn:pers$personId snvoc:birthday $birthday .
    sn:pers$personId snvoc:creationDate $creationDate .
    sn:pers$personId snvoc:locationIP $locationIP .
    sn:pers$personId snvoc:browserUsed $browserUsed .
    sn:pers$personId snvoc:isLocatedIn ?city .
}
WHERE
{
    ?city a dbpedia-owl:City .
    ?city snvoc:id $cityId
}
